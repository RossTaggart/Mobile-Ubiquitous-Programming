<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.mondayschild" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mcOutputScreen"
            android:label="Day Born"></activity>
        <activity android:name=".mcSaveDataOutput"
            android:label="@string/mc_SavedData"></activity>
        <activity android:name=".mcBioActivity"
            android:label="Bio Activity"></activity>
    </application>

</manifest>
